{
    "mode": "melotts-es-es-axcl",
    "type": "tts",
    "homepage": "https://github.com/ml-inory/melotts.axera/tree/main/model_convert",
    "compile_flage": "pulsar2 build --input decoder-es.onnx --config config_decoder_u16_es-es.json --output_dir decoder-es-axcl --output_name decoder-es-es.axmodel --target_hardware AX650 --npu_mode NPU3 --compiler.check 0",
    "pulsar_version": "4.0-64a0e58f",
    "capabilities": [
        "tts",
        "Spanish"
    ],
    "input_type": [
        "tts.utf-8"
    ],
    "output_type": [
        "tts.wav",
        "sys.play.0_1"
    ],
    "mode_param": {
        "encoder": "encoder-es-es.ort",
        "decoder": "decoder-es-es.axmodel",
        "gbin": "g-es.bin",
        "tokens": "tokens-es.txt",
        "lexicon": "lexicon-es.txt",
        "spacker_speed": 1.2,
        "mode_rate": 44100,
        "audio_rate": 48000,
        "awake_delay": 1000
    }
}